    "fluent": "^0.10.0",
    "helmet": "^3.15.0",
    "instrumental-agent": "^2.3.0",
    "moment-timezone": "^0.5.23",
    "node-http-error": "^2.0.0",
    "on-finished": "^2.3.0",
    "snake-case": "^2.1.0",
    "themeparks": "^5.0.0",
    "title-case": "^2.1.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "codacy-coverage": "^3.4.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "json-sort-cli": "^1.11.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.2.0",
    "prettier": "^1.16.0",
    "sinon": "^7.2.3",
    "sort-package-json": "^1.17.1"
